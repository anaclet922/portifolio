{
    "builds": [{
        "src": "my_app/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.11" }
    }],
    "routes": [
        {
            "src": "/static/(.*)",
            "headers": {
                "Content-Type": "text/css"
            }
        },
        {
            "src": "/(.*)",
            "dest": "my_app/wsgi.py"
        }
    ]
}
